    "practical_analysis": {
      "name": "实战分析模板",
      "name_en": "Practical Analysis Template",
      "lessons": [
        {
          "id": "analysis_001",
          "title": "店铺数据分析模板",
          "content": "系统分析店铺数据，找出优化方向。",
          "analysis_template": {
            "traffic_analysis": {
              "metrics": ["曝光量", "点击量", "点击率(CTR)"],
              "questions": [
                "流量来源分布是否合理？",
                "搜索流量占比是否足够？",
                "点击率是否达到行业平均？",
                "流量趋势是上升还是下降？"
              ]
            },
            "inquiry_analysis": {
              "metrics": ["询盘数", "询盘转化率", "询盘质量"],
              "questions": [
                "询盘数是否稳定增长？",
                "哪些产品询盘最多？",
                "询盘转化率是否达标？",
                "询盘来源国家分布？"
              ]
            },
            "product_analysis": {
              "metrics": ["产品数量", "优品数量", "产品活跃度"],
              "questions": [
                "产品数量是否足够？",
                "优品占比是多少？",
                "有多少沉默产品需要激活？",
                "产品覆盖的关键词是否全面？"
              ]
            }
          }
        },
        {
